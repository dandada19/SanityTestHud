<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox" stylesheets="@sidestage.css, @bootstrap3.css" fx:controller="dzhu.controller.PretQtpController">

	<Label wrapText="true" text="Run qtp tests and check the results in your outlook" />
	<HBox styleClass="hbox">
		<Button fx:id="btnPretQtpRetailIc" text="Retail IC" onAction="#btnPretQtpRetailIcClicked"/>
		<CheckBox />
	</HBox>
	<HBox styleClass="hbox">
		<Button fx:id="btnPretQtpLondonAsiaRegionIcLogin" text="LondonAsiaRegion IC Login" onAction="#btnPretQtpLondonAsiaRegionIcLoginClicked"/>
		<CheckBox />
	</HBox>
	<HBox styleClass="hbox">
		<Button fx:id="btnPretQtpE2EELogin" text="E2EE Login" onAction="#btnPretQtpE2EELoginClicked"/>
		<CheckBox />
	</HBox>
	<HBox styleClass="hbox">
		<Button fx:id="btnPretQtpRetailMargin" text="Retail Margin" onAction="#btnPretQtpRetailMarginClicked"/>
		<CheckBox />
	</HBox>
	<HBox styleClass="hbox">
		<Button fx:id="btnPretQtpRetailWs" text="Retail WS" onAction="#btnPretQtpRetailWsClicked"/>
		<CheckBox />
	</HBox>
	<HBox styleClass="hbox" visible="false">
		<Button fx:id="btnPretQtpRetailAdminTool" text="Retail Admin Tool" onAction="#btnPretQtpRetailAdminToolClicked"/>
		<CheckBox />
	</HBox>
</VBox>