<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox"
		stylesheets="@sidestage.css, @bootstrap3.css" 
		fx:controller="dzhu.controller.BretWebAdminLoginController">

	<Label text="Successfully Logon to WebAdmin" />
	<Label fx:id="lb1" text="    User Name: [#BretSettings.WEBADMIN_USERNAME#]" />
	<Label fx:id="lb2" text="    Password: [#BretSettings.WEBADMIN_PASSWORD#]" />
	<Label text="    Cert: Required" />
	<Label wrapText="true" text="After click logon button below, you may need manually select the cert and click Allow button on popup." />
	<Button fx:id="btnBretLogonWebAdmin" text="Logon WebAdmin" onAction="#btnBretLogonWebAdminClicked"/>
	<Button fx:id="btnBretLaunchWebAdmin" text="Launch WebAdmin" onAction="#btnBretLaunchWebAdminClicked"/>
	<Label wrapText="true" text="Make sure you logon to webadmin before click below button" />
	<Button fx:id="btnBretResetPin" text="Reset Pin for Enroll" onAction="#btnBretResetPinClicked"/>
	<Label fx:id="lbBretWebadminResult" wrapText="true" text="" />
</VBox>