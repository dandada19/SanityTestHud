<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox" stylesheets="@sidestage.css, @bootstrap3.css" fx:controller="dzhu.controller.ProdX2Controller">

	<Label wrapText="true" text="1. Run Selenium X2 Test" />
	<Button fx:id="btnProdSeleniumX2Test" text="Selenium X2 Tests" onAction="#btnProdSeleniumX2TestClicked"/>
	<Label wrapText="true" text="2. If Selemnium test failed, contact Rahul." />
	<Label wrapText="true" text="3. Or you can manually login to X2 and do some trades" />
	<Label fx:id="lb1" text="    User Name: [#ProdSettings.X2TAKER_USERNAME#]" />
	<Label fx:id="lb2" text="    Password: [#ProdSettings.X2TAKER_PASSWORD#]" />
	<Label text="    Cert: required" />
	<Button fx:id="btnProdLogonX2" text="Logon X2" onAction="#btnProdLogonX2Clicked"/>
</VBox>