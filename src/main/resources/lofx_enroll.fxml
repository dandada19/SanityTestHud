<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox" stylesheets="@sidestage.css, @bootstrap3.css" fx:controller="dzhu.controller.LofxEnrollController">
	
	<Label wrapText="true" text="1. You should have reset pin of your enroll acc on WebAdmin " />
	<Label fx:id="lb1" text="    User Name: [#LofxSettings.ENROLLMENT_USERNAME#]" />
	<Label fx:id="lb2" text="    Password: [#LofxSettings.ENROLLMENT_PASSWORD#]"/>
	<Label wrapText="true" text="2. Click below button to enroll" />
	<Button fx:id="btnLofxEnroll" text="Enroll" onAction="#btnLofxEnrollClicked"/>
	<Label wrapText="true" text="3. Go to IE and remove old cert" />
	<Button fx:id="btnLofxLaunchIe" text="Launch IE" onAction="#btnLofxLaunchIeClicked"/>
</VBox>