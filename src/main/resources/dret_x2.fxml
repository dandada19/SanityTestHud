<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox" stylesheets="@sidestage.css, @bootstrap3.css" fx:controller="dzhu.controller.DretX2Controller">

	<Label wrapText="true" text="1. Run Selenium X2 Test" />
	<Button fx:id="btnDretSeleniumX2Test" text="Selenium X2 Tests" onAction="#btnDretSeleniumX2TestClicked"/>
	<Label wrapText="true" text="2. If Selemnium test failed, contact Rahul." />
	<Label wrapText="true" text="3. Or you can manually login to X2 and do some trades" />
	<Label fx:id="lb1" text="    User Name: [#DretSettings.X2TAKER_USERNAME#]" />
	<Label fx:id="lb2" text="    Password: [#DretSettings.X2TAKER_PASSWORD#]" />
	<Label text="    Cert: required" />
	<Button fx:id="btnDretLaunchX2" text="Launch X2" onAction="#btnDretLaunchX2Clicked"/>
</VBox>