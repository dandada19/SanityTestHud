<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox"
		stylesheets="@sidestage.css, @bootstrap3.css" 
		fx:controller="dzhu.controller.PretWebAdminLoginController">

	<Label text="Successfully Logon to WebAdmin" />
	<Label fx:id="lb1" text="    User Name: [#PretSettings.WEBADMIN_USERNAME#]" />
	<Label fx:id="lb2" text="    Password: [#PretSettings.WEBADMIN_PASSWORD#]" />
	<Label text="    Cert: Required" />
	<Label wrapText="true" text="After click logon button below, you may need manually select the cert and click Allow button on popup." />
	<Button fx:id="btnPretLogonWebAdmin" text="Logon WebAdmin" onAction="#btnPretLogonWebAdminClicked"/>
	<Button fx:id="btnPretLaunchWebAdmin" text="Launch WebAdmin" onAction="#btnPretLaunchWebAdminClicked"/>
	<Label wrapText="true" text="Make sure you logon to webadmin before click below button" />
	<Button fx:id="btnPretResetPin" text="Reset Pin for Enroll" onAction="#btnPretResetPinClicked"/>
	<Label fx:id="lbPretWebadminResult" wrapText="true" text="" />
</VBox>