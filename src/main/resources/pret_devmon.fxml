<VBox xmlns:fx="http://javafx.com/fxml" styleClass="vbox" stylesheets="@sidestage.css, @bootstrap3.css" fx:controller="dzhu.controller.PretDevmonController">

	<Label text="1. Login DEVMON with your user" />
	<Label text="2. Check all nodes are  green" />
	<Label text="    -devmon printNodeStatus to get all nodes" />
	<Label text="3. Compare active orders" />
	<Button fx:id="btnPretCompareOrders" text="Compare Orders" onAction="#btnPretCompareOrdersClicked"/>
	<Button fx:id="btnPretLaunchDevmon" text="Launch DEVMON" onAction="#btnPretLaunchDevmonClicked"/>
	<Button fx:id="btnPretLogonDevmon" text="Logon DEVMON" onAction="#btnPretLogonDevmonClicked"/>
</VBox>